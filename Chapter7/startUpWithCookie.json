[
  {
    "description":"这是一个会返回cookies信息的get请求",
    "request":{
      "uri":"/getCookies",
      "method":"get"
    },
    "response":{
      "cookies":{
        "login":"true"
      },
      "text":"恭喜获得cookies信息",
      "headers":{
        "Content-Type":"text/html;charset=gbk"
      }
    }
  },
  {
    "description":"模拟一个带cookies信息的get请求",
    "request":{
      "uri":"/get/with/cookies",
      "method":"get",
      "cookies":{
        "login":"true"
      }
    },
    "response":{
      "text":"这是一个需要携带cookies信息的get请求",
      "headers":{
        "Content-Type":"text/html;charset=gbk"
      }
    }
  },
  {
    "description":"这是一个带cookies参数的post请求",
    "request":{
      "uri":"/get/with/cookies",
      "method":"post",
      "cookies":{
        "login":"true"
      },
      "json":{
        "name":"huhansan",
        "sex":"man"
      }
    },
    "response":{
      "status":200,
      "json":{
        "huhansan":"胡汉三带参数来了！！！",
        "status":1
      },
      "headers":{
        "Content-Type":"text/html;charset=gbk"
      }

    }
  },

  {
    "description":"这是一个带cookies参数的post请求",
    "request":{
      "uri":"/post/with/cookies",
      "method":"post",
      "cookies":{
        "login":"true"
      },
      "json":{
        "name":"huhansan",
        "sex":"man"
      }
    },
    "response":{
      "status":200,
      "json":{
        "huhansan":"success",
        "status":"1"
      },
      "headers":{
        "Content-Type":"text/html;charset=gbk"
      }

    }
  }

]