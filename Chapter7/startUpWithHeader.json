[
  {
    "description":"这是一个带header参数的post请求",
    "request":{
      "uri":"/post/with/headers",
      "method":"post",
      "headers":{
        "content-type":"application/json"
      },
      "json":{
        "name":"wanglaosi",
        "sex":"woman"
      }
    },
    "response":{
      "json":{
        "wanglaosi":"success"
      }
    }
  },
  {
    "description":"这是一个带cookies参数的post请求",
    "request":{
      "uri":"/get/with/cookies",
      "method":"post",
      "cookies":{
        "login":"true"
      },
      "json":{
        "name":"huhansan",
        "sex":"man"
      }
    },
    "response":{
      "status":200,
      "json":{
        "huhansan":"胡汉三带参数来了！！！",
        "success":1
      }

    }
  }

]